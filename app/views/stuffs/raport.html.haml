%h1 Wybierz szczegóły raportu

= form_for :list, :url => {:controller => 'stuffs', :action => 'raport'} do |f|
  .form-group 
    = f.label :type_id
    = f.collection_select :type_id, Type.all, :id, :name, prompt: true
  .form-group
    = f.label :room_id
    = f.collection_select :room_id, Room.all, :id, :name, prompt: true
  .form-group
    = f.label :funding_id
    = f.collection_select :funding_id, Funding.all, :id, :name, prompt: true
  .form-group
    = f.label :user_id
    = f.collection_select :user_id, User.all, :id, :lastname, prompt: true

  .form-actions
    = f.button :submit

%br
%br
%br



%table.table
  %tr
    - @stuffs.columns.each do |columns|
      %th
        = columns
    - @stuffs.rows.each do |rows|
      %tr
        - rows.each do |tom|
          %td
            = tom

%br


